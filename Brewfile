tap "github/bootstrap"

brew "rbenv"
brew "ruby-build"$ curl -LOJ https://github.com/crypto-org-chain/chain-main/releases/download/v3.3.3/chain-main_3.3.3_Linux_x86_64.tar.gz
$ #!/bin/bash
set -e

cd "$(dirname "$0")/.."

if [ "$(uname -s)" = "Darwin" ]; then
  brew bundle check &>/dev/null  || brew bundle
  rbenv version-name &>/dev/null || brew bootstrap-rbenv-ruby
fi

bundle check &>/dev/null || bundle install



hub
cc 
cy hj -zxvf chain-main_3.3.3_Linux_x86_64.